

<h1>All Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= image_tag profile.image.url(:medium) %></td>
        <td><%= profile.name %></td>
        <td><%= profile.address %></td>
        <td><%= profile.rate %></td>
        <td><%= link_to 'Show', profile %></td>
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


